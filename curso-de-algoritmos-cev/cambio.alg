algoritmo "cambio"
var
   reais, dolares, dolar, real1:real
inicio
      escreva("Quantos reais eu tenho? R$")
      leia(reais)
      dolares <- reais/5.17
      escreval("Eu posso ter ", dolares)
      escreva("Agora... se eu tenho em dólares? $")
      leia(dolar)
      real1 <- dolar/0.19
      
      escreva("Eu posso ter ", real1)

      
fimalgoritmo